{
  "fields": [
    {
      "name": "space_id",
      "description": "The space_id of the space to which the entity belongs to.",
      "type": "string",
      "constraints": {
        "required": true,
        "unique": true,
        "maxLength": 6,
        "pattern": "SP[0-9]{4}"
      }
    },
    {
      "name": "old_id",
      "description": "id from previous id schema",
      "type": "string",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "space_type",
      "description": "PL for place, L for locations (with NULL coordinates)",
      "type": "string",
      "constraints": {
        "required": true,
        "pattern": "PL|L"
      }
    },
    {
      "name": "space_name",
      "description": "The name of the spatial entity.",
      "type": "string",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "language",
      "description": "structural entry for key resolution",
      "type": "string",
      "constraints": {
        "required": true,
        "pattern": "en"
      }
    },
    {
      "name": "lat",
      "type": "number",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "long",
      "type": "number",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "wikidata_id",
      "description": "Wikidata id of spatial entity",
      "type": "string",
      "constraints": {
        "required": false,
        "pattern": "^(Q[0-9]+)*"
      }
    },
    {
      "name": "note",
      "description": "structural entry for internal notes",
      "type": "string",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "created",
      "description": "structural entry creation date of entry",
      "type": "date",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "created_by",
      "description": "structural entry acronym of project member creating entry",
      "type": "string",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}|ReadActor"
      }
    },
    {
      "name": "last_modified",
      "description": "structural entry last modification date",
      "type": "date",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "last_modified_by",
      "description": "structural entry acronym of project member last modifying entry",
      "type": "string",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}|ReadActor"
      }
    }
  ],
  "primaryKey": "space_id",
  "missingValues": [
    ""
  ],
  "foreignKeys": [
    {
      "fields": "language",
      "reference": {
        "resource": "iso-lang-code",
        "fields": "iso_code"
      }
    }
  ]
}