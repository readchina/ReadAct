{
  "fields": [{
      "name": "inst_id",
      "constraints": {
        "required": true,
        "unique": false,
        "max-length": 6,
        "pattern": "AG[0-9]{4}"
      }
    },
    {
      "name": "inst_name",
      "constraints": {
        "required": true
      }
    },
    {
      "name": "inst_name_lang",
      "constraints": {
        "required": true,
        "pattern": "[a-z]{2}"
      }
    }, {
      "name": "place",
      "constraints": {
        "required": false,
        "pattern": "(^SP[0-9]{4})"
      }
    }, {
      "name": "start",
      "constraints": {
        "required": false,
        "min-length": 4
      }
    }, {
      "name": "end",
      "constraints": {
        "required": false,
        "min-length": 4
      }
    }, {
      "name": "alt_start",
      "constraints": {
        "required": false,
        "min-length": 4
      }
    }, {
      "name": "alt_end",
      "constraints": {
        "required": false,
        "min-length": 4
      }
    }, {
      "name": "inst_alt_name",
      "constraints": {
        "required": false
      }
    }, {
      "name": "note",
      "description": "structural entry for internal notes",
      "constraints": {
        "required": false
      }
    }, {
      "name": "source",
      "constraints": {
        "required": false
      }
    }, {
      "name": "page",
      "constraints": {
        "required": false
      }
    },
    {
      "name": "created",
      "description": "structural entry creation date of entry",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#date"
      }
    },
    {
      "name": "created_by",
      "description": "structural entry acronym of project member creating entry",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}"
      }
    },
    {
      "name": "last_modified",
      "description": "structural entry last modification date",
      "constraints": {
        "required": true,
        "type": "http://www.w3.org/2001/XMLSchema#date"
      }
    },
    {
      "name": "last_modified_by",
      "description": "structural entry acronym of project member last modifying entry",
      "constraints": {
        "required": true,
        "pattern": "[A-Z]{2}"
      }
    }
  ],
  "primaryKey": ["inst_id", "inst_name", "inst_name_lang"],
  "missingValues": ["", "NULL"],
  "foreignKeys": [
    {
      "fields": "inst_id",
      "reference": {
        "resource": "agent",
        "fields": "agent_id"
      }
    },
    {
      "fields": "place",
      "reference": {
        "resource": "space",
        "fields": "space_id"
      }
    },
    {
      "fields": "inst_name_lang",
      "reference": {
        "resource": "iso-lang-code",
        "fields": "iso_code"
      }
    }
  ]
}
